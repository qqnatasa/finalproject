{% trans_default_domain 'FOSUserBundle' %}

{% block body %}
<h1>Register</h1>
<div class="in">
{{ form_start(form, {'method': 'post', 'action':
 path('fos_user_registration_register'), 'attr':
 {'class': 'fos_user_registration_register'}}) }}
